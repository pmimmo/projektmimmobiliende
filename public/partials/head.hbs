<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1, viewport-fit=cover" />
<title>{{title}}</title>

{{#if keywords}}<meta name="keywords" content="{{keywords}}">{{/if}}
<meta name="author" content="Projekt M Immobilien GmbH">
{{#if description}}<meta name="description" content="{{description}}">{{/if}}
{{#if canonical}}<link rel="canonical" href="{{canonical}}">{{/if}}

<!-- Favicon -->
<link rel="icon" href="/img/favicon.ico" sizes="any">
<link rel="icon" type="image/png" sizes="32x32" href="/img/favicon-32x32.png">
<link rel="icon" type="image/png" sizes="16x16" href="/img/favicon-16x16.png">
<link rel="apple-touch-icon" href="/img/apple-touch-icon.png">

{{!-- <script src="https://cloud.ccm19.de/app.js?apiKey=eb7f5702b2f40ea96cae74f1e61cbbf192153d960092c72c&amp;domain=689e1b0546961f2e8b062142" referrerpolicy="origin"></script> --}}

<!-- Open Graph Meta-Tags -->
<meta property="og:title" content="{{title}}">
{{#if description}}<meta property="og:description" content="{{description}}">{{/if}}
<meta property="og:type" content="website">
{{#if canonical}}<meta property="og:url" content="{{canonical}}">{{/if}}
{{#if image}}<meta property="image" content="{{image}}">{{/if}}
<meta property="og:site_name" content="Projekt M Immobilien GmbH">

<!-- Twitter Meta Tags -->
<meta name="twitter:card" content="summary_large_image">
{{#if title}}<meta name="twitter:title" content="{{title}}">{{/if}}
{{#if description}}<meta name="twitter:description" content="{{description}}">{{/if}}
{{#if image}}<meta name="twitter:image" content="{{image}}">{{/if}}
{{!-- {{#if twitter.site}}<meta name="twitter:site" content="{{twitter.site}}">{{/if}} --}}

 <!-- Structured Data (JSON-LD) -->
<script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "RealEstateAgent",
        "name": "{{author}}",
        "image": "https://projektmimmobilien.de/img/logo.png",
        "description": "{{description}}",
        "address": {
            "@type": "PostalAddress",
            "streetAddress": "Krumbacherstraße 5",
            "addressLocality": "München",
            "postalCode": "80798",
            "addressCountry": "DE"
        },
        "url": "{{canonical}}",
        "telephone": "+49-89-28806768"
    }
</script>

